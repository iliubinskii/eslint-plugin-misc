{
  "name": "eslint-plugin-misc",
  "version": "3.0.0",
  "private": false,
  "description": "ESLint plugin",
  "keywords": [
    "eslint",
    "plugin",
    "rules",
    "typescript"
  ],
  "homepage": "https://github.com/iliubinskii/eslint-plugin-misc",
  "bugs": {
    "url": "https://github.com/iliubinskii/eslint-plugin-misc/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/iliubinskii/eslint-plugin-misc.git"
  },
  "license": "ISC",
  "author": "Ilia Liubinskii <ilia.liubinskii@gmail.com> (https://github.com/iliubinskii)",
  "main": "dist/index.js",
  "module": "es/index.js",
  "files": [
    "/dist",
    "/es",
    "/src"
  ],
  "scripts": {
    "prepare-next-version": "run-s type-check format lint test build-commonjs build-es build-doc && git add . && git commit -m \"build: Prepare next version\"",
    "build-commonjs": "cf dist && tsc --project tsconfig-build.json",
    "build-doc": "cf docs && tsx utils/build-doc.ts && cpy assets/docs/static/* docs",
    "build-es": "cf es && tsc --project tsconfig-build-es.json",
    "format": "prettier --write \"**/*.{css,html,json,less,postcss,scss}\"",
    "install-deps": "npm install --no-audit --save-prod --strict-peer-deps",
    "install-deps-clean": "npm install-clean --no-audit --save-prod --strict-peer-deps",
    "install-dev-deps": "npm install --no-audit --save-dev --strict-peer-deps",
    "install-dev-deps-clean": "npm install-clean --no-audit --save-dev --strict-peer-deps",
    "lint": "eslint --fix --max-warnings=0 .",
    "lint-commit-msg": "commitlint --edit",
    "lint-markdown": "markdownlint *.md",
    "lint-no-fix": "eslint --max-warnings=0 .",
    "lint-profile": "cross-env TIMING=1 eslint --max-warnings=0 .",
    "ncu-check": "ncu --dep=dev,peer,prod",
    "ncu-upgrade": "ncu --dep=dev,peer,prod --upgrade && npm run install-deps",
    "prepare": "husky",
    "test": "cf coverage/lcov-report && jest --coverage",
    "type-check": "tsc --incremental false --noEmit",
    "update": "npm update --no-audit --strict-peer-deps"
  },
  "lint-staged": {
    "*.{css,html,json,less,postcss,scss}": "prettier --write",
    "*.{js,jsx,ts,tsx},!dist/**,!es/**": "eslint --max-warnings=0",
    "*.md": "markdownlint"
  },
  "dependencies": {
    "@types/estree": "^1.0.5",
    "@types/minimatch": "^5.1.2",
    "@types/natural-compare": "^1.4.3",
    "javascript-stringify": "^2.1.0",
    "lodash-commonjs-es": "^1.0.9",
    "minimatch": "^9.0.4",
    "natural-compare": "^1.4.0",
    "tslib": "^2.6.3",
    "tsutils": "^3.21.0",
    "typescript-misc": "^1.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@jest/expect-utils": "^29.7.0",
    "@jest/test-sequencer": "^29.7.0",
    "@types/eslint": "^8.56.10",
    "@types/jest": "^29.5.12",
    "@types/micromatch": "^4.0.9",
    "@types/node": "^20.14.8",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "babel-jest": "^29.7.0",
    "clear-folder": "^5.0.0",
    "cpy-cli": "^5.0.0",
    "cross-env": "^7.0.3",
    "css-property-sort-order-smacss": "^2.2.0",
    "del-cli": "^5.1.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-config-strict": "^14.0.1",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-css-modules": "^2.12.0",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-plugin-es": "^4.1.0",
    "eslint-plugin-escompat": "^3.4.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-etc": "^2.0.3",
    "eslint-plugin-github": "^5.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-jest-extended": "^2.4.0",
    "eslint-plugin-jsdoc": "^48.4.0",
    "eslint-plugin-misc": "^3.0.0",
    "eslint-plugin-no-type-assertion": "^1.3.0",
    "eslint-plugin-no-use-extend-native": "^0.5.0",
    "eslint-plugin-no-useless-assign": "^1.0.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-pii": "^1.0.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.2.0",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-native": "^4.1.0",
    "eslint-plugin-regexp": "^2.6.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^0.25.1",
    "eslint-plugin-sort": "^3.0.2",
    "eslint-plugin-sort-annotation": "^1.0.7",
    "eslint-plugin-sort-destructure-keys": "^2.0.0",
    "eslint-plugin-sort-exports": "^0.9.1",
    "eslint-plugin-sort-imports-requires": "^1.0.2",
    "eslint-plugin-spellcheck": "^0.0.20",
    "eslint-plugin-typescript-sort-keys": "^3.2.0",
    "eslint-plugin-unicorn": "^54.0.0",
    "eslint-plugin-unused-imports": "^3.2.0",
    "eslint-plugin-xss": "^0.1.12",
    "get-current-line": "^7.4.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jest-matcher-utils": "^29.7.0",
    "jest-transform-stub": "^2.0.0",
    "markdownlint-cli": "^0.41.0",
    "micromatch": "^4.0.7",
    "npm-check-updates": "^16.14.20",
    "npm-package-json-lint": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-html": "^1.7.0",
    "prettier": "^3.3.2",
    "prettier-plugin-sort-json": "^4.0.0",
    "strip-ansi-cli": "^4.0.0",
    "ts-jest": "^29.1.5",
    "tsx": "^4.11.0",
    "typedoc": "^0.26.2",
    "typedoc-plugin-markdown": "^4.1.0"
  },
  "peerDependencies": {
    "@typescript-eslint/parser": "^7",
    "@typescript-eslint/utils": "^7",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
