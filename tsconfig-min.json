{
  "extends": "real-config/tsconfig/min",
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": ".cache/typescript",
    "paths": {
      "@": [
        "src"
      ],
      "@/*": [
        "src/*"
      ]
    },
    "typeRoots": [
      "node_modules/@types",
      "node_modules/types-fix"
    ],
    "types": [
      "jest",
      "node",
      "typescript-any-to-unknown"
    ]
  },
  "exclude": [
    ".cache",
    "coverage",
    "dist",
    "docs",
    "es",
    "node_modules"
  ],
  "include": [
    "*.js",
    "*.ts",
    ".eslintrc.js",
    ".eslintrc.overrides.js",
    ".eslintrc.rule-overrides.js",
    ".eslintrc.synonyms.js",
    ".eslintrc.temp.js",
    ".postcssrc.js",
    ".prettierrc.js",
    ".stylelintrc.js",
    ".stylelintrc-html.js",
    "fixtures/**/*.js",
    "fixtures/**/*.ts",
    "src/**/*.js",
    "src/**/*.ts",
    "tests/**/*.js",
    "tests/**/*.ts"
  ]
}
